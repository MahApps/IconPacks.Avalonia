<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconVaadinIcons Kind="Abacus" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconVaadinIcons.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconVaadinIcons" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconVaadinIcons}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconVaadinIcons.Theme}"
                  TargetType="iconPacks:PackIconVaadinIcons" />

</ResourceDictionary>