<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconFileIcons Kind="_1c" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconFileIcons.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconFileIcons" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconFileIcons}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconFileIcons.Theme}"
                  TargetType="iconPacks:PackIconFileIcons" />

</ResourceDictionary>