<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconZondicons Kind="AddOutline" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconZondicons.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconZondicons" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconZondicons}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconZondicons.Theme}"
                  TargetType="iconPacks:PackIconZondicons" />

</ResourceDictionary>