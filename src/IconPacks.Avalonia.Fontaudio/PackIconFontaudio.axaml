<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconFontaudio Kind="Adr" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconFontaudio.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconFontaudio" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconFontaudio}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconFontaudio.Theme}"
                  TargetType="iconPacks:PackIconFontaudio" />

</ResourceDictionary>