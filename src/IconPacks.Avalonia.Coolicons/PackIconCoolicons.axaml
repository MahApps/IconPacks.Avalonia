<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconCoolicons Kind="AddColumn" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconCoolicons.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconCoolicons" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconCoolicons}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconCoolicons.Theme}"
                  TargetType="iconPacks:PackIconCoolicons" />

</ResourceDictionary>