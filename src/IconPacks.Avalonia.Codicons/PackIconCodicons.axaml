<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconCodicons Kind="Account" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconCodicons.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconCodicons" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconCodicons}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconCodicons.Theme}"
                  TargetType="iconPacks:PackIconCodicons" />

</ResourceDictionary>