<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconMaterialDesign Kind="_10k" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconMaterialDesign.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconMaterialDesign" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconMaterialDesign}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconMaterialDesign.Theme}"
                  TargetType="iconPacks:PackIconMaterialDesign" />

</ResourceDictionary>