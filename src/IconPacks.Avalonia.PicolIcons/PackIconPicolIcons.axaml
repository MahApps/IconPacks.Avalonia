<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconPicolIcons Kind="Accept" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconPicolIcons.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconPicolIcons" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconPicolIcons}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconPicolIcons.Theme}"
                  TargetType="iconPacks:PackIconPicolIcons" />

</ResourceDictionary>