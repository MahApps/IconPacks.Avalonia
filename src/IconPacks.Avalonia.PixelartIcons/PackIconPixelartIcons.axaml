<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconPixelartIcons Kind="_4g" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconPixelartIcons.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconPixelartIcons" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconPixelartIcons}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconPixelartIcons.Theme}"
                  TargetType="iconPacks:PackIconPixelartIcons" />

</ResourceDictionary>