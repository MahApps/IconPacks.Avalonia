<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:iconPacks="https://github.com/MahApps/IconPacks.Avalonia"
                    x:ClassModifier="internal">

    <Design.PreviewWith>
        <iconPacks:PackIconFontAwesome Kind="_0Solid" />
    </Design.PreviewWith>

    <ControlTheme x:Key="IconPacks.Avalonia.PackIconFontAwesome.Theme"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconControlBase.Theme}"
                  TargetType="iconPacks:PackIconFontAwesome" />

    <ControlTheme x:Key="{x:Type iconPacks:PackIconFontAwesome}"
                  BasedOn="{StaticResource IconPacks.Avalonia.PackIconFontAwesome.Theme}"
                  TargetType="iconPacks:PackIconFontAwesome" />

</ResourceDictionary>